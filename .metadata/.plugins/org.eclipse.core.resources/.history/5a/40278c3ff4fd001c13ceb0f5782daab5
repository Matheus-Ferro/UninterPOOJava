package empresa;

public class Funcionario {
	private String nome;
	
	public Funcionario(String nome) {
		super();
		this.nome = nome;
	}

	public String getNome() {
		return nome;
	}

	public void setNome(String nome) {
		this.nome = nome;
	}

	public float pagamento() {
		System.out.println("Processando Pagamento...");
		return 0.0f;
	}
}
