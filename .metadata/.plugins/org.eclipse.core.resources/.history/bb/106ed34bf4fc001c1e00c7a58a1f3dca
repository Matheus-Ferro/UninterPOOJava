package empresa;

public class Cofrinho {
	Moeda moe;
	double total;
	public Cofrinho(Moeda moe){
		setMoe(moe);
	}
	
	public Moeda getMoe() {
		return moe;
	}

	public void setMoe(Moeda moe) {
		total += moe.getValor();
		this.moe = moe;
	}

	public double calcularTotal() {
		return this.moe;
	}
	
}
